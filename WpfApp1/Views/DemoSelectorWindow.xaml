<Window x:Class="WpfApp1.Views.DemoSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Demo Selector" Height="150" Width="350" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">
        <Button Margin="0,0,0,10" Command="{Binding OpenBindingsViewCommand}">Bindings</Button>
        <Button Margin="0,0,0,10" Command="{Binding OpenTabViewCommand}">Tabs</Button>
        <Button Margin="0,0,0,10" Command="{Binding OpenArticleListViewCommand}">Articles</Button>
        <TextBlock Text="{Binding OpenBindingsViewCommand}"></TextBlock>
        <TextBlock Text="{Binding}"></TextBlock>
    </StackPanel>
</Window>
